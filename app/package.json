{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:dotnet && vite",
    "build": "npm run publish:dotnet && tsc && vite build",
    "clean:dotnet": "rimraf ./public/assets",
    "build:dotnet": "npm run clean:dotnet && dotnet build ../octostache/OctostacheWrapper.csproj",
